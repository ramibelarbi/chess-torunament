<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ADD8E6;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chess.oublierController">
    <children>
        <ToolBar prefHeight="36.0" prefWidth="695.0">
            <items>
                <Button fx:id="btnBack" mnemonicParsing="false" onAction="#GoToPrincipal" text="&lt;">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
            </items>
        </ToolBar>
        <Label fx:id="wronglogin" layoutX="238.0" layoutY="362.0" prefHeight="18.0" prefWidth="4.0" />
        <TextField fx:id="Nom" layoutX="215.0" layoutY="151.0" prefHeight="30.0" prefWidth="234.0" />
        <Label layoutX="227.0" layoutY="133.0" text="Nom" />
        <Label layoutX="224.0" layoutY="191.0" text="Adress" />
        <TextField fx:id="txtadress" layoutX="215.0" layoutY="211.0" prefHeight="30.0" prefWidth="234.0" />
        <Text fill="#711817" layoutX="195.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="mot de passe oublier">
            <font>
                <Font size="30.0" />
            </font>
        </Text>
      <TextField fx:id="pass" layoutX="217.0" layoutY="322.0" prefHeight="30.0" prefWidth="234.0" />
      <Label layoutX="224.0" layoutY="298.0" text="le mot de passe est :" />
